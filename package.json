{
  "name": "boilerplate-react",
  "version": "0.0.1",
  "description": "My boilerplate for react",
  "main": "index.js",
  "scripts": {
    "flow-check": "flow check",
    "docs": "esdoc -c esdoc.json",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "start": "NODE_ENV=development node server.js",
    "test": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/_mocha --require ./test/.jsdom.setup.js 'test/**/*_test.js'",
    "test-all": "npm run flow-check && npm test",
    "tdd": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/_mocha --require ./test/.jsdom.setup.js 'test/**/*_test.js' --watch"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Fabrice Bentz <fabrice.bentz@weeple.fr>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "esdoc": "^0.4.8",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "postcss": "^5.1.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "postcss-url": "^5.1.2",
    "react-addons-test-utils": "^15.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbentz/boilerplate-react.git"
  },
  "bugs": {
    "url": "https://github.com/fbentz/boilerplate-react/issues"
  },
  "homepage": "https://github.com/fbentz/boilerplate-react#readme"
}
